{
  "name": "TGR Emoji Extension",
  "description": "Fixes an Issue with the Emoji Numbers on the TGR Website",
  "version": "1.0",
  "manifest_version": 3,

  "content_scripts":
  [
    {
      "matches": ["https://www.tetongravity.com/*"],
      "all_frames": true,
      "js": ["content_script.js"],
      "run_at": "document_end"} 
  ]
}
